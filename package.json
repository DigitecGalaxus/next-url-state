{
  "name": "next-url-state",
  "version": "0.1.0",
  "description": "React hooks for managing state in URL search parameters with Next.js",
  "author": "Michael Holzner <michael.holzner@digitecgalaxus.ch>",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "react",
    "hooks",
    "url",
    "query-params",
    "search-params",
    "state-management",
    "routing"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/next-url-state.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/next-url-state/issues"
  },
  "homepage": "https://github.com/yourusername/next-url-state#readme",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "next": ">=12.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "esbuild": "^0.25.10",
    "eslint": "^8.56.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "next": "^14.0.0",
    "ts-jest": "^29.4.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
